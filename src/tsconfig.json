{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "baseUrl": "./src",
    "allowJs": true,
    "paths": {
      "*": ["types/*"]
    },
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "compileOnSave": true,
  "include": [
    "src", "global.d.ts",  "./src/**/*.ts", "custom.d.ts",
  ],
  "exclude":[]
}
